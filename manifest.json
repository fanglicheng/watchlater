{
  "manifest_version": 2,

  "name": "WatchLater",
  "description": "",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["http://*/*",
                  "https://*/*"],
      "js": ["contentscript.js"]
    }
  ],

  "browser_action": {
    "default_icon": "pacman128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
